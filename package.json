    "@faker-js/faker": "^9.2.0",
    "@types/aws-serverless-express": "^3.3.10",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^16.11.10",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.2.5",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "10.9.1",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.15.0"
  },
  "dependencies": {
    "aws-serverless-express": "^3.4.0",
    "axios": "^1.7.7",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "install": "^0.13.0",
    "mysql": "^2.14.1",
    "npm": "^10.9.1",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "0.3.20"
  },
  "scripts": {
    "start": "nodemon src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --log-level warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint:check-prettier": "prettier  --log-level warn --check \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint:check-eslint": "eslint ./src --ext .jsx,.js,.ts,.tsx --max-warnings 0 --ignore-path ./.gitignore",
    "lint": "npm run lint:format && npm run lint:fix",
    "test": "jest",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d ./src/infrastructure/database/mysql-local-ds.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d ./src/infrastructure/database/mysql-local-ds.ts ./src/infrastructure/database/migrations/migrations",
    "build": "tsc"
  }
